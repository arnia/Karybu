<query id="insertOrder" action="insert">
    <tables>
        <table name="shop_orders" />
    </tables>
    <columns>
        <column name="order_srl" var="order_srl" filter="number" notnull="notnull" />
        <column name="cart_srl" var="cart_srl" filter="number" />
        <column name="module_srl" var="module_srl" filter="number" notnull="notnull" />
        <column name="member_srl" var="member_srl" filter="number" />
        <column name="client_name" var="client_name"/>
        <column name="client_email" var="client_email"/>
        <column name="client_company" var="client_company"/>
        <column name="billing_address" var="billing_address"/>
        <column name="shipping_address" var="shipping_address"/>
        <column name="payment_method" var="payment_method"/>
        <column name="shipping_method" var="shipping_method"/>
        <column name="shipping_variant" var="shipping_variant"/>
        <column name="shipping_cost" var="shipping_cost" />
        <column name="total" var="total"/>
        <column name="vat" var="vat" filter="number"/>
        <column name="order_status" var="order_status"/>
        <column name="transaction_id" var="transaction_id" />
        <column name="ip" var="ip"/>
        <column name="discount_min_order" var="discount_min_order" />
        <column name="discount_type" var="discount_type" />
        <column name="discount_amount" var="discount_amount" />
        <column name="discount_tax_phase" var="discount_tax_phase" />
        <column name="discount_reduction_value" var="discount_reduction_value" />
        <column name="currency" var="currency" />
        <column name="regdate" var="regdate" default="curdate()"/>
    </columns>
</query>
