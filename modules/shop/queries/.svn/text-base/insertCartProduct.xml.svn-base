<query id="insertCartProduct" action="insert">
    <tables>
        <table name="shop_cart_products" />
    </tables>
    <columns>
        <column name="cart_srl" var="cart_srl" filter="number" notnull="notnull"/>
        <column name="product_srl" var="product_srl" filter="number" notnull="notnull"/>
        <column name="quantity" var="quantity" default="1" />
        <column name="title" var="title" size="255" />
        <column name="price" var="price" />
    </columns>
</query>