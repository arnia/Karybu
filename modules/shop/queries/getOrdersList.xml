<query id="getOrdersList" action="select">
    <tables>
        <table name="shop_orders" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
        <condition operation="equal" column="module_srl" var="module_srl" notnull="notnull"/>
        <condition operation="equal" column="member_srl" var="member_srl" pipe="and"/>
        <condition operation="like" column="billing_address" var="billing_address" pipe="and"/>
        <condition operation="like" column="shipping_address" var="shipping_address" pipe="and"/>
    </conditions>
    <navigation>
        <index var="order_srl" default="order_srl" order="order_type" />
        <list_count var="list_count" default="10" />
        <page_count var="page_count" default="10" />
        <page var="page" default="1" />
    </navigation>
</query>
