<table name="shop_shipping_methods">
    <column name="id" type="number" size="3" notnull="notnull" primary_key="primary_key" />
    <column name="module_srl" type="number" size="11" notnull="notnull" unique="unique_module_srl_name" />
    <column name="name" type="varchar" size="255" notnull="notnull" unique="unique_module_srl_name"/>
    <column name="display_name" type="varchar" size="255" notnull="notnull" />
    <column name="status" type="number" size="1" default="0" />
    <column name="is_default" type="number" size="1" default="0" />
    <column name="props" type="text" />
</table>
